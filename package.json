{
  "name": "unnester",
  "version": "1.0.1",
  "description": "a library to unnest objects",
  "keywords": [
    "javascript",
    "typescript",
    "unnest",
    "object"
  ],
  "author": "Fulvio Coelho",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "homepage": "",
  "main": "build/index.js",
  "types": "build/index.d.js",
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test ts-mocha",
    "test:w": "nodemon --watch \"app/**\" --ext \"ts\" --exec \"yarn test\"",
    "test:cov": "cross-env NODE_ENV=test nyc ts-mocha",
    "test:cw": "nodemon --watch \"./**\" --ext \"ts\" --exec \"yarn test:cov\"",
    "dev-start": "ts-node-dev src/index.ts",
    "dev-start:w": "nodemon --watch \"src/**\" --ext \"ts\" --exec \"yarn dev-start\"",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "chai": "^4.3.7",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "^2.8.1",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "directories": {
    "test": "tests"
  },
  "license": "ISC"
}
